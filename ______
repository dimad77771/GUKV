+<script type="text/javascript" language="javascript">
+	// <![CDATA[
+
+	var reportId = <%= ReportID %>;
+	var allPoints = <%= Converter.JsonConvertSerializeObject(AllPoints) %>;
+	//console.log("allPoints", allPoints);
+	var selected_fs_id = <%= selected_fs_id %>;
+
