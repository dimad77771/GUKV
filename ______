+						if (apoint.need_zgoda == '+') {
+							html_need_zgoda =
+								'<tr>' +
+								'<td colspan="2" style="text-align:left; border:1px solid; padding:5px"><span style="color:#FFD926; font-weight:bold">Потребує погодження</span></td>' +
+								'</tr>';
+						}
