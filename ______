+		AdjustGridSizes();
+	};
+
+	function AdjustGridSizes() {
+		//console.log(window.innerHeight);
+		//alert(window.innerHeight);
+		$("#map").height(window.innerHeight - 180);
+		$("#map").width(window.innerWidth - 20);
+		//$("#map").height(600);
+		//console.log($("#map"));
+		//alert(reportId);
