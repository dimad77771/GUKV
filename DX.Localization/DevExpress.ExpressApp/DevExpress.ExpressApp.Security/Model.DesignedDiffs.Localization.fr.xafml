      <LocalizationItem Name="Navigate" Value="Naviguer" />
      <LocalizationItem Name="Read" Value="Lecture" />
      <LocalizationItem Name="Write" Value="Écriture" />
    </LocalizationGroup>
    <LocalizationGroup Name="Namespaces">
      <LocalizationItem Name="DevExpress.ExpressApp.Security" Value="Sécurité" />
    </LocalizationGroup>
  </Localization>
  <NavigationItems>
    <Items>
      <Item Id="Default">
        <Items>
          <Item Id="MyDetails" Caption="Mes détails" />
        </Items>
      </Item>
    </Items>
  </NavigationItems>
  <Views>
    <DetailView Id="AuthenticationStandardLogonParameters_DetailView" Caption="Connexion">
      <Items>
        <StaticText Id="LogonText" Text="Bienvenue! Veuillez saisir vos nom d'utilisateur et mots de passe ci-dessous." />
      </Items>
      <Layout>
        <LayoutGroup Id="Main" Caption="Principal">
          <LayoutGroup Id="RootGroup">
            <LayoutGroup Id="EditorsGroup">
              <LayoutGroup Id="Editors" Caption="Editeurs" />
            </LayoutGroup>
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="ChangePasswordOnLogonParameters_DetailView" Caption="Modifier le mot de passe lors de la prochaine connexion">
      <Items>
        <StaticText Id="ChangePasswordText" Text="Pour des raisons de sécurité, vous devez définir votre mot de passe. Veuillez entrer votre mot de passe dans les 2 champs ci-dessous. Ne réutilisez pas un ancien mot de passe." />
      </Items>
      <Layout>
        <LayoutGroup Id="Main" Caption="Principal">
          <LayoutGroup Id="Static" Caption="Statique" />
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="ChangePasswordParameters_DetailView" Caption="Modifier le mot de passe">
      <Items>
        <StaticText Id="ChangePasswordText" Text="Pour modifier votre mot de passe, veuillez entrer votre ancien puis votre nouveau mot de passe dans les champs ci-dessous." />
      </Items>
      <Layout>
        <LayoutGroup Id="Main" Caption="Principal">
          <LayoutGroup Id="Static" Caption="Statique" />
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="EditModelPermission_DetailView">
      <Layout>
        <LayoutGroup Id="Main" Caption="Principal" />
      </Layout>
    </DetailView>
    <DetailView Id="ObjectAccessPermission_DetailView">
      <Layout>
        <LayoutGroup Id="Main" Caption="Principal">
          <LayoutGroup Id="SimpleEditors">
            <LayoutGroup Id="ObjectAccessPermission" Caption="Droit d'accès à l'objet" />
          </LayoutGroup>
        </LayoutGroup>
      </Layout>
    </DetailView>
    <DetailView Id="ResetPasswordParameters_DetailView" Caption="Réinitialiser le mot de passe">
      <Items>
        <StaticText Id="ResetPasswordText" Text="La définition du mot de passe de l'utilisateur va créer un nouveau mot de passe automatiquement. Vous devrez communiquer le nouveau mot de passe à l'utilisateur pour qu'il puisse continuer à utiliser l'application. Voulez-vous continuer ?" />
      </Items>
      <Layout>
        <LayoutGroup Id="Main" Caption="Principal">
          <LayoutGroup Id="Static" Caption="Statique" />
        </LayoutGroup>
      </Layout>
    </DetailView>
  </Views>
</Application>