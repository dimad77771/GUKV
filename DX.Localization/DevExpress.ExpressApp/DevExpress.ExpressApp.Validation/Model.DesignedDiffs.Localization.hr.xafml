  <Validation>
    <ErrorMessageTemplates>
      <RuleBase MessageTemplateSkipNullOrEmptyValues="Pravilo &quot;{Id}&quot; nije izvršeno jer nisu određena svojstva ciljnog objekta." MessageTemplateTargetDoesNotSatisfyTargetCriteria="Pravilo &quot;{Id}&quot; nije izvršeno jer ciljni objekta ne udovoljava kriteriju ciljnog objekta pravila." />
      <RuleCollectionProperty MessageTemplateTargetDoesNotSatisfyCollectionCriteria="Pravilo &quot;{Id}&quot; nije izvršeno jer ciljni objekt nije element kolekcije &quot;{TargetCollectionOwnerType}.{TargetCollectionPropertyName}&quot;." MessageTemplateCollectionValidationMessageSuffix="(Za elemente kolekcije &quot;{TargetCollectionOwnerType}.{TargetCollectionPropertyName}&quot;)." />
      <RuleCombinationOfPropertiesIsUnique MessageTemplateCombinationOfPropertiesMustBeUnique="Kombinacija sljedećih svojstava mora biti jedinstvena: {FormattedTargetProperties}." MessageTemplateFoundObjects="Pronađeni su sljedeći objekti: " />
      <RuleCriteria MessageTemplateMustSatisfyCriteria="Objekt &quot;{TargetObject}&quot; mora udovoljavati sljedećim uvjetim: &quot;{Criteria}&quot;." />
      <RuleFromBoolProperty MessageTemplateMustBeTrue="&quot;{TargetPropertyName}&quot; mora biti postavljen na 'true'." />
      <RuleIsReferenced MessageTemplateMustBeReferenced="Objekt &quot;{TargetObject}&quot; mora biti referenciran." MessageTemplateFoundObjects="Pronađeni su sljedeći objekti: " />
      <RuleObjectExists MessageTemplateMustExist="Mora postojati objekt koji udovoljava uvjetu &quot;{Criteria}&quot;." MessageTemplateFoundObjects="Pronađeni su sljedeći objekti: " />
      <RuleRange MessageTemplateMustBeInRange="&quot;{TargetPropertyName}&quot; mora biti u rasponu između &quot;{MinimumValue}&quot; i &quot;{MaximumValue}&quot;, uključujući i granice." />
      <RuleRegularExpression MessageTemplateMustMatchPattern="&quot;{TargetPropertyName}&quot; mora odgovarati sljedećem uzorku: &quot;{Pattern}&quot;." />
      <RuleRequiredField MessageTemplateMustNotBeEmpty="&quot;{TargetPropertyName}&quot; ne smije biti prazno." />
      <RuleSearchObject MessageTemplateFoundObjects="Pronađeni su sljedeći objekti: " />
      <RuleStringComparison MessageTemplateMustContain="&quot;{TargetPropertyName}&quot; mora sadržavati &quot;{OperandValue}&quot;." MessageTemplateMustBeginWith="&quot;{TargetPropertyName}&quot; mora početi s &quot;{OperandValue}&quot;." MessageTemplateMustEndWith="&quot;{TargetPropertyName}&quot; mora završavati s &quot;{OperandValue}&quot;." MessageTemplateMustBeEqual="&quot;{TargetPropertyName}&quot; mora biti jednak &quot;{OperandValue}&quot;." MessageTemplateMustNotBeEqual="&quot;{TargetPropertyName}&quot; mora biti različit od &quot;{OperandValue}&quot;." />
      <RuleUniqueValue MessageTemplateMustBeUnique="Postoji objekt s istom vrijednošću za &quot;{TargetPropertyName}&quot;. &quot;{TargetPropertyName}&quot; vrijednosti moraju biti jedinstvene." />
      <RuleValueComparison MessageTemplateMustBeEqualToOperand="&quot;{TargetPropertyName}&quot; mora biti jednak &quot;{RightOperand}&quot;." MessageTemplateMustBeGreaterThanOperand="&quot;{TargetPropertyName}&quot; mora biti veći od &quot;{RightOperand}&quot;." MessageTemplateMustBeGreaterThanOrEqualToOperand="&quot;{TargetPropertyName}&quot; mora biti veći ili jednak &quot;{RightOperand}&quot;." MessageTemplateMustBeLessThanOperand="&quot;{TargetPropertyName}&quot; mora biti manji od &quot;{RightOperand}&quot;." MessageTemplateMustBeLessThanOrEqualToOperand="&quot;{TargetPropertyName}&quot; mora biti manji ili jednak od &quot;{RightOperand}&quot;." MessageTemplateMustNotBeEqualToOperand="&quot;{TargetPropertyName}&quot; mora biti različit od &quot;{RightOperand}&quot;." />
    </ErrorMessageTemplates>
