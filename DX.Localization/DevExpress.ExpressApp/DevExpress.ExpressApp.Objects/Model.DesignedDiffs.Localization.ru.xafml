			<RuleFromBoolProperty Id="HCategoryCircularReferences" CustomMessageTemplate="Обнаружена циклическая ссылка. Чтобы исправить эту ошибку, задайте другое значение свойству 'Предок'." Name="Циклические ссылки иерархических категорий" />
			<RuleUniqueValue Id="PropertyDescriptor Code is unique" CustomMessageTemplate="Значение 'Код' должно быть уникальным" Name="Код описания свойства уникален" FoundObjectMessagesSeparator="," />
			<RuleRequiredField Id="PropertyDescriptor Code required" Name="Требуется код описания свойства" />
			<RuleCriteria Id="Role is not referenced" CustomMessageTemplate="Нельзя удалить роль, так как есть пользователи которым она отведена" Name="Нет ссылки на роль" MessageTemplateMustSatisfyCriteria="Объект &quot;{TargetObject}&quot; должен удовлетворять следующему критерию: &quot;{Criteria}&quot;." />
			<RuleRequiredField Id="Role required" CustomMessageTemplate="Активный пользователь должен иметь как минимум одну отведенную ему роль" Name="Требуется задать роль" />
			<RuleUniqueValue Id="User Name is unique" CustomMessageTemplate="Пользователь с введенным именем уже зарегистрирован в системе" Name="Имя пользователя является уникальным" FoundObjectMessagesSeparator="," />
			<RuleRequiredField Id="User Name required" CustomMessageTemplate="Имя пользователя не должно быть пустым" Name="Требуется задать имя пользователя" />
		</Rules>
	</Validation>
