<?xml version="1.0" encoding="utf-8"?>
<Application>
  <ActionDesign>
    <Actions>
      <Action Id="ChooseTheme" Caption="Vybrat téma" />
      <Action Id="Edit" Caption="Upravit" />
      <Action Id="InlineEdit" Caption="Řádková editace" />
      <Action Id="NavigateTo" Caption="Přejít na" />
      <Action Id="QuickCreateAction" Caption="Rychle vytvořit" />
      <Action Id="SwitchToEditMode" Caption="Upravit" />
    </Actions>
  </ActionDesign>
  <BOModel>
    <Class Name="DevExpress.ExpressApp.Web.ParameterlessLogonFailedInfo">
      <OwnMembers>
        <Member Name="Message" Caption="Zpráva" />
      </OwnMembers>
    </Class>
  </BOModel>
  <Localization>
    <LocalizationGroup Name="Messages">
      <LocalizationItem Name="ThereAreNoRecordsToDisplay" Value="Žádné záznamy k zobrazení." />
    </LocalizationGroup>
  </Localization>
  <Views>
    <DetailView Id="AboutInfo_DetailView">
      <Items>
        <StaticText Id="AboutText" Text="{0:ProductName}&lt;br&gt;{0:Version}&lt;br&gt;&lt;br&gt;{0:Copyright}&lt;br&gt;&lt;br&gt;{0:Description}" />
      </Items>
    </DetailView>
    <DetailView Id="LogonAttemptsAmountedToLimit_DetailView" Caption="Přihlásit">
      <Items>
        <StaticText Id="LogonText" Text="Po třech neúspěšných pokusech o přihlášení bude prohlížeč zavřen. Kontaktujte administrátora s informací o detailech přihlášení." />
      </Items>
    </DetailView>
    <DetailView Id="ParameterlessLogonFailedInfo_DefaultDetailView" Caption="Přihlásit">
      <Items>
        <StaticText Id="Message" Text="{0} Kontaktujte administrátora s informací o detailech přihlášení." />
      </Items>
    </DetailView>
  </Views>
</Application>