<?xml version="1.0" encoding="utf-8"?>
<Application>
  <ActionDesign>
    <Actions>
      <Action Id="ChooseTheme" Caption="Escolha o Tema" />
      <Action Id="Edit" Caption="Editar" />
      <Action Id="InlineEdit" Caption="Edição direta" />
      <Action Id="NavigateTo" Caption="Navegar para" />
      <Action Id="QuickCreateAction" Caption="Criação rápida" />
      <Action Id="SwitchToEditMode" Caption="Editar" />
    </Actions>
  </ActionDesign>
  <BOModel>
    <Class Name="DevExpress.ExpressApp.Web.LogonAttemptsAmountedToLimit" Caption="Tentativas de Logon atingiram o limite" />
    <Class Name="DevExpress.ExpressApp.Web.ParameterlessLogonFailedInfo" Caption="Informações de Falha do Logon sem parâmetros">
      <OwnMembers>
        <Member Name="Message" Caption="Mensagem" />
      </OwnMembers>
    </Class>
  </BOModel>
  <Localization>
    <LocalizationGroup Name="Messages">
      <LocalizationItem Name="ThereAreNoRecordsToDisplay" Value="Não há registros a mostrar" />
    </LocalizationGroup>
  </Localization>
  <Views>
    <DetailView Id="AboutInfo_DetailView">
      <Items>
        <StaticText Id="AboutText" Text="{0:ProductName}&lt;br&gt;{0:Version}&lt;br&gt;&lt;br&gt;{0:Copyright}&lt;br&gt;&lt;br&gt;{0:Description}" />
      </Items>
    </DetailView>
    <DetailView Id="LogonAttemptsAmountedToLimit_DetailView" Caption="Fazer Log On">
      <Items>
        <StaticText Id="LogonText" Text="Depois de três tentativas de login sem sucesso, o navegador deve ser fechado. Por favor, considere entrar em contato com o administrador do sistema sobre seus dados de login." />
      </Items>
    </DetailView>
    <DetailView Id="ParameterlessLogonFailedInfo_DefaultDetailView" Caption="Fazer Log On">
      <Items>
        <StaticText Id="Message" Text="{0} Por favor, considere entrar em contato com o administrador do sistema sobre seus dados de login." />
      </Items>
    </DetailView>
  </Views>
</Application>