<?xml version="1.0" encoding="utf-8"?>
<Application>
  <ActionDesign>
    <Actions>
      <Action Id="ChooseTheme" Caption="Wybierz skórkę" />
      <Action Id="Edit" Caption="Edytuj" />
      <Action Id="InlineEdit" Caption="Edytuj inline" />
      <Action Id="NavigateTo" Caption="Przejdź do" />
      <Action Id="QuickCreateAction" Caption="Szybkie tworzenie" />
      <Action Id="SwitchToEditMode" Caption="Edytuj" />
    </Actions>
  </ActionDesign>
  <BOModel>
    <Class Name="DevExpress.ExpressApp.Web.LogonAttemptsAmountedToLimit" Caption="Limit prób logowania" />
    <Class Name="DevExpress.ExpressApp.Web.ParameterlessLogonFailedInfo">
      <OwnMembers>
        <Member Name="Message" Caption="Wiadomość" />
      </OwnMembers>
    </Class>
  </BOModel>
  <Localization>
    <LocalizationGroup Name="Messages">
      <LocalizationItem Name="ThereAreNoRecordsToDisplay" Value="Brak rekordów do wyświetlenia." />
    </LocalizationGroup>
  </Localization>
  <Views>
    <DetailView Id="AboutInfo_DetailView">
      <Items>
        <StaticText Id="AboutText" Text="{0:ProductName}&lt;br&gt;{0:Version}&lt;br&gt;&lt;br&gt;{0:Copyright}&lt;br&gt;&lt;br&gt;{0:Description}" />
      </Items>
    </DetailView>
    <DetailView Id="LogonAttemptsAmountedToLimit_DetailView" Caption="Zaloguj się">
      <Items>
        <StaticText Id="LogonText" Text="Po trzech nieudanych próbach logowania, przeglądarka powinna zostać zamknięta. Proszę rozważyć skontaktowanie się z administratorem o dane logowania." />
      </Items>
    </DetailView>
    <DetailView Id="ParameterlessLogonFailedInfo_DefaultDetailView" Caption="Zaloguj się">
      <Items>
        <StaticText Id="Message" Text="{0} Proszę rozważyć skontaktowanie się z administratorem o dane logowania." />
      </Items>
    </DetailView>
  </Views>
</Application>